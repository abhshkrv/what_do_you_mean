<h1>
  Add a new result
</h1>


<hr>

<!-- Form result start -->
<form action="/create_result" method="post">
<!-- Form result end -->

  <!-- Label and input for submission_id start -->
  <div class="form-group">
    <label for="submission_id">
      Submission
    </label>

    <!-- Input for submission_id start -->
    <input type="text" id="submission_id" name="submission_id" class="form-control">
    <!-- Input for submission_id end -->
  </div>
  <!-- Label and input for submission_id end -->

  <!-- Label and input for flags start -->
  <div class="form-group">
    <label for="flags">
      Flags
    </label>

    <!-- Input for flags start -->
    <input type="text" id="flags" name="flags" class="form-control">
    <!-- Input for flags end -->
  </div>
  <!-- Label and input for flags end -->

  <!-- Label and input for confidence start -->
  <div class="form-group">
    <label for="confidence">
      Confidence
    </label>

    <!-- Input for confidence start -->
    <input type="text" id="confidence" name="confidence" class="form-control">
    <!-- Input for confidence end -->
  </div>
  <!-- Label and input for confidence end -->

  <button class="btn btn-block btn-outline-secondary">
    Create result
  </button>
</form>

<hr>

<a href="/results">
  Go back
</a>
